{
    "entry": {
        "all": {
            "fileTypes": [],
            "format": "browser"
        }
        , "module": {
            "return": "$$container$$"
            , "moduleFile": "module.json"
        }
        , "test": {
            "strict": false
            , "naming": false
            , "lint": false
            , "files": [
              "+./*.spec.js"
            ]
            , "includes": {
                "postAssembler": null
            }
        }
        , "node": {
            "minify": false
            , "strict": true
            , "lint": false
            , "global": "global"
        }
        , "browser": {
            "minify": true
            , "strict": true
            , "lint": false
            , "global": "window"
        }
        , "none": {
            "minify": false
            , "strict": true
            , "lint": false
        }
        , "routing": {
            "files": [
              "+./*.route.js"
              , "{repos}/TruJS/log/_Reporter.js"
            ]
            , "moduleFile": "route.module.json"
            , "hints": {
                "TruJS": "{repos}/TruJS"
            }
            , "module": {
                "root": [{"TruJS":[":TruJS"]}]
                , "promise": [":Promise"]
                , "global": [":global"]
                , "performance": ["TruJS.timing._Performance", []]
                , "funcAsync": ["TruJS.func._Async", []]
            }
        }
    }
    , "collector": {
        "encoding": {
            ".js": "utf8"
            , ".css": "utf8"
            , ".html": "utf8"
            , ".json": "utf8"
            , ".txt": "utf8"
            , ".sass": "utf8"
            , ".scss": "utf8"
            , ".xml": "utf8"
            , ".svg": "utf8"
            , ".base64": "utf8"
        }
    }
    , "module": {
        "iocPaths": [
          "{repos}/TruJS/TruJS.js"
          , "{repos}/TruJS/_Object.js"
          , "{repos}/TruJS/func/_Inspector.js"
          , "{repos}/TruJS/ioc/_Entry.js"
          , "{repos}/TruJS/ioc/_Container.js"
          , "{repos}/TruJS/ioc/Container.js"
        ]
    }
    , "minifier": {
        "extentionMap": {
            ".js": "javascript"
            , ".css": "css"
            , ".html": "html"
        }
        , "javascript": {
            "mangle": true
        }
    }
    , "test": {
        "testFile": "test.json"
        , "testType": "test"
    }
    , "manifest": {
        "manifestFile": "manifest.json"
        , "manifestDir": "{script}/manifest.json"
    }
    , "scriptsDir": "scripts"
    , "fileTypes": {
        ".html": "Html"
        , ".scss":"Scss"
        , ".css": "Css"
        , ".json": "Json"
        , ".xml": "Xml"
        , ".txt": "Txt"
        , ".svg": "Svg"
        , ".png": "Png"
        , ".base64": "Base64"
    }
}